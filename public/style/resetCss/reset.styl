* {
    margin: 0;
    padding: 0;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
html,
body {
     min-width: 350px;
    overflow-x: hidden;
    background-color:#fdfffc;
    color: #011627;
    font-size: 20px;
    font-family: "Bai Jamjuree", sans-serif;
    text-rendering: optimizeLegibility;
}


